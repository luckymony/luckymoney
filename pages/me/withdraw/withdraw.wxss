.content{
  margin-top: 4.5vw;
  margin-left: 4.5vw;
  margin-right: 4.5vw; 
  width: 91vw;
  height: 60vh;
  border-radius: 10rpx;
  background-color: #fff;
  box-shadow: 5px 5px 5px gainsboro;
  display: flex;
  flex-direction:column;
}

.head-view {
  height: 70%;
  /* background-color: orange; */
}

.foot-view {
  height: 30%;
  display: flex;
  flex-direction: column;
  /* background-color: olivedrab; */
}

.button-view {
  /* background-color: rebeccapurple; */
  flex: 2;
}

.tixian-tip{
  width: 100%;
  text-align: center;
  font-size: 25rpx;
  color: #ccc;
  /* background-color: red; */
}

.yu-e{
  /* background-color: orange; */
  margin-top: 40rpx;
  margin-left: 10vw;
  margin-right: 10vw;
  display: flex;
  flex-direction: row;
  align-items: center;
}

.yu-e-name{
  flex: 1;
  font-size: 35rpx;
}

.yu-e-money{
  font-size: 50rpx;
  font-weight: bolder;
}

.ti-xian-jin-e{
  /* background-color: yellowgreen; */
  margin-top: 40rpx;
  margin-left: 10vw;
  margin-right: 10vw;
  font-size: 35rpx;
}

.jin-e-input-view {
  margin-top: 40rpx;
  margin-left: 10vw;
  margin-right: 10vw;
  display: flex;
  flex-direction: row;
  /* background-color: red; */
}

.jin-e-input-view text {
  height: 160rpx;
  font-size: 100rpx;
  font-weight: bolder;
}

.jin-e-input {
  margin-left: 40rpx;
  font-size: 100rpx;
  font-weight: bolder;
  width: 100%;
  height: 150rpx;
  /* background-color: orange; */
}

.background-line{
  background-color: #eee;
  height: 1rpx;
  margin-left: 10vw;
  margin-right: 10vw;
}

.quan-bu-ti-xian{
  /* background-color: orange; */
  margin-top: 20rpx;
  margin-left: 10vw;
  margin-right: 10vw;
  font-size: 25rpx;
  color: #ccc;
}

.all-ti-xian {
  margin-left: 20rpx;
  color: blue;
}

.service-fee-text {
  margin-top: 20rpx;
  text-align: center;
  margin-left: 10vw;
  color: rgba(16, 16, 16, 0.43);
  font-size: 25rpx;
}

.withdrawal-button {
  width: 80%;
  font-size: 40rpx;
  background:#EB5646;
  color: white; 
}

.gray {
  background-color: #eee;
}

.more-than-view {
  margin-top: 20rpx;
  margin-left: 10vw;
  margin-right: 10vw;
  font-size: 25rpx;
  color: #F44848;
}

.content button {
  margin-top: 40rpx;
  margin-left: 10vw;
  margin-right: 10vw;
}



